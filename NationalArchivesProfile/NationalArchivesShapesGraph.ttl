## SHAPES GRAPH 
## UNDER UDVIKLING (development)

:ITSystemShape 
    a sh:NodeShape ;
    sh:targetClass sys:ITSystemShape ;

    sh:property [
        sh:path skos:prefLabel ;
	      sh:languageIn ( "da" ) ;
    ] ;        
    sh:property [
        sh:path sys:hasImplementation ;
        sh:node :ImplementedITSystemShape ;        
    ] .
    
:ImplementedITSystemShape 
    a sh:NodeShape ;
    sh:targetClass sys:ImplementedITSystem ;

    sh:property [
        sh:path sys:hasImplementation ;
        sh:node :OrganizationShape ;
    ] .    


:DatasetShape
    a sh:NodeShape ;
    sh:targetClass dcat:Dataset ;

    sh:property [
      sh:path dc:title;
      sh:languageIn ( "da" ) ;
      sh:name "Titel" ;
      sh:description "Datasættets formelle navn" ;
      sh:minCount 1 ;
    ] ;

    sh:property [
      sh:path dc:description;
      sh:name "Beskrivelse" ;
      sh:description "Beskrivelse af datasættets formål og indhold ";
      sh:languageIn ( "da" ) ;
      sh:minCount 1 ;
    ] ;

    sh:property [
      sh:path dcatdk:dataResopnsibleOrganisation ;
      sh:name "Dataansvarlig" ;
      sh:node :OrganizationShape ;
    ] ;

    sh:property [
      sh:path dcatdk:personalDataCategory ;
      sh:name "Personoplysningskategori" ;   
      sh:nodeKind sh:IRI ;             
    ] ;

    sh:property [
      sh:path dc:subject ;
      sh:name "Forvaltningsopgave" ; 
    ] .

:OrganizationShape 
    a sh:NodeShape ;
    sh:targetClass foaf:Organization ;

    sh:property [
        sh:path foaf:name ;
	      sh:languageIn ( "da" ) ;
    ] .
